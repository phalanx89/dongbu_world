<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
		<display-name>dongbu_world</display-name>
		<context-param>
				<param-name>CONTEXT_PATH</param-name>
				<param-value>/dongbu_world</param-value>
		</context-param>
		<welcome-file-list>
				<welcome-file>index.jsp</welcome-file>
		</welcome-file-list>
		<servlet>
				<description></description>
				<display-name>FoodController</display-name>
				<servlet-name>FoodController</servlet-name>
				<servlet-class>work.controller.FoodController</servlet-class>
		</servlet>
		<servlet-mapping>
				<servlet-name>FoodController</servlet-name>
				<url-pattern>/food_controller</url-pattern>
		</servlet-mapping>
		<servlet>
				<servlet-name>FrontController</servlet-name>
				<servlet-class>work.controller.FrontController</servlet-class>
		</servlet>
		<servlet-mapping>
				<servlet-name>FrontController</servlet-name>
				<url-pattern>/controller</url-pattern>
		</servlet-mapping>
</web-app>